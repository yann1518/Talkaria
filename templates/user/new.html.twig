{% extends 'base.html.twig' %}

{% block title %}Créer un compte{% endblock %}

{% block content %}
    <h1>Créer un compte</h1>
    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        <button class="btn btn-primary">Créer mon compte</button>
    {{ form_end(form) }}
{% endblock %}